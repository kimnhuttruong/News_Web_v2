<!-- page content -->
<div class="right_col" role="main">
  <div class="">
    <div class="page-title">
      <div class="title_left">
        <h3>Danh sách bài viết</h3>
      </div>

    
    </div>

    <div class="clearfix"></div>

    <div class="row">
      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
          <div class="x_title">
            <ul class="nav navbar-right panel_toolbox">
              <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
              </li>
             
              <li><a class="close-link"><i class="fa fa-close"></i></a>
              </li>
            </ul>
          </div>
          <div class="x_content">

            <div class="clearfix">
              <div class="input-group mb-3">
              <div class="input-group-prepend">
                <label class="input-group-text" for="inputGroupSelect01"></label>
              </div>
              <form action="/account/writer/news" method="get">
                <label class="input-group-text" for="inputGroupSelect01">Status</label>
                <select class="custom-select" id="statusid" name="statusid">
                  <option selected value="-1">All</option>
                  {{!-- {{#each statuses}}
                  <option {{#equalTopic id ../statusid}}{{/equalTopic}} value="1"> {{id}} {{isdelete}}</option>
                  {{/each}} --}}
                  {{!-- <option {{#equalTopic "-1" statusid}}{{/equalTopic}} value="-1"> All</option> --}}
                  <option {{#equalTopic "1"  statusid}}{{/equalTopic}} value="1"> Đã được duyệt & chờ xuất bản</option>
                  <option {{#equalTopic "2"  statusid}}{{/equalTopic}} value="2"> Đã xuất bản</option>
                  <option {{#equalTopic "3"  statusid}}{{/equalTopic}} value="3"> Bị từ chối</option>
                  <option {{#equalTopic "4"  statusid}}{{/equalTopic}} value="4"> Chưa được duyệt</option>
                </select>

                <button type="submit" class="btn btn-success btn-sm">Xem</button>
              </form>
            </div>
            </div>

            </br>
            <table class="js-dynamitable     table table-bordered">
              <thead>
                <tr>
                  <th>Chuyên mục</th>
                  <th>Chủ đề</th>
                  <th>Tiêu đề</th>
                  <th>Mô tả</th>
                  <th>Ngày đăng</th>
                  <th>Trạng thái</th>
                  <th>Premium</th>
                  {{!-- <th>Lượt xem</th> --}}
                  {{!-- <th>Tags</th> --}}
                  <th>Thao tác </th>
                </tr>
              </thead>
              <tbody>
                {{#each list}}
                <tr>
                  <td>{{category}}</td>
                  <td>{{topic}}</td>
                  <td>{{name}}</td>
                  <td>{{desc}}</td>
                  <td>{{time}}</td>
                  <td>{{statusNespaper statusid}}</td>
                  <td>{{ispremium}}</td>
                  <td class="text-right">
                    <a class="btn btn-primary btn-sm" href="./view/preview/{{id}}" role="button" title="preview">
                      <i class="fa fa-eye" aria-hidden="true"></i></a>
                      {{!-- <a class="btn btn-primary btn-sm" href="./view/preview_avatar/{{id}}" role="button" title="avatar">
                      <i class="fa fa-image" aria-hidden="true"></i> --}}
                    </a>
                    {{#if (allowUpdate statusid)}}
                    <a class="btn btn-primary btn-sm" href="./news/update/{{id}}" role="button" title="edit">
                      <i class="fa fa-pencil" aria-hidden="true"></i>
                    </a>
                    <a class="btn btn-danger btn-sm" href="./news/delete/{{id}}" role="button" title="delete">
                      <i class="fa fa-trash" aria-hidden="true"></i>
                    </a>
                    {{/if}}
                  </td>
                </tr>
                {{/each}}
              </tbody>
            </table>
            <nav aria-label="Page navigation example">
                        <ul class="pagination justify-content-center">
                            <li class="page-item {{prev.css}}"><a class="page-link" onclick="{{prev.onclick}}"
                                    href="{{origin}}&page=1">First</a></li>
                            <li class="page-item {{prev.css}}">
                                <a class="page-link " onclick="{{prev.onclick}}"
                                    href="{{origin}}&page={{#add page -1}}{{/add}}"
                                    tabindex="-1">Previous</a>
                            </li>
                            {{#each pages}}
                            <li class="page-item {{active}}"><a class="page-link"
                                    href="{{../origin}}&page={{index}}">{{index}}</a></li>
                            {{/each}}
                            <li class="page-item {{next.css}}">
                                <a class="page-link " onclick="{{next.onclick}}"
                                    href="{{origin}}&page={{#add page 1}}{{/add}}">Next</a>
                            </li>
                            <li class="page-item {{next.css}}"><a class="page-link" onclick="{{next.onclick}}"
                                    href="{{origin}}&page={{last}}">Last</a></li>
                        </ul>
                    </nav>
            {{!-- <nav aria-label="Page navigation example">
              <ul class="pagination justify-content-end">
                <li class="page-item ">
                  <a class="page-link" href="/account/writer/news?page={{prepage}}" {{#if prepage}} disabled {{/if}}
