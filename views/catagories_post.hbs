<link rel="stylesheet" href="../../public/css/master.css" />
<link rel="stylesheet" href="/public/style.css">
<header class="header-area">
    <!-- Top Header Area -->
    <div class="top-header-area">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="top-header-content d-flex align-items-center justify-content-between">
                        <!-- Logo -->
                        <div class="logo">
                            <a href="/"><img src="../../public/img/core-img/logo.png" alt=""></a>
                        </div>

                        <!-- Login Search Area -->
                        <div class="login-search-area d-flex align-items-center">
                            <!-- Login -->
                            <div class="login d-flex">
                                <a href="/account/{{login}}">
                                    <img src="../../public/img/core-img/no_image.png" alt=""
                                        style="width:30px;height:30px;">{{upper_login login}}</a>
                                <a  href="/account/{{#role user.roleid}}{{/role}}" >
                                    {{#if isURL}}<img src="{{user.img}}" alt="" style="width:30px;height:30px;">
                                    {{else}}<img src="{{user.img}}" alt=""
                                        style="width:30px;height:30px;">{{/if}}{{user.username}}</a>

                            </div>
                            <!-- Search Form -->
                            <div class="search-form">
                                <form action="/newspaper/search" method="get">
                                    <input type="search" class="typeahead tt-query" name="typeahead" placeholder="">
                                    <button type="submit"><i class="fa fa-search" aria-hidden="true"></i></button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Navbar Area -->
    <div class="newspaper-main-menu" id="stickyMenu">
        <div class="classy-nav-container breakpoint-off">
            <div class="container">
                <!-- Menu -->
                <nav class="classy-navbar justify-content-between" id="newspaperNav">

                    <!-- Logo -->
                    <div class="logo">
                        <a href="/"><img src="public/img/core-img/logo.png" alt=""></a>
                    </div>

                    <!-- Navbar Toggler -->
                    <div class="classy-navbar-toggler">
                        <span class="navbarToggler"><span></span><span></span><span></span></span>
                    </div>

                    <!-- Menu -->
                    <div class="classy-menu">

                        <!-- close btn -->
                        <div class="classycloseIcon">
                            <div class="cross-wrap"><span class="top"></span><span class="bottom"></span></div>
                        </div>

                        <!-- Nav Start -->
                        <nav class="navbar navbar-expand-md">
                            <div class="container">

                                <button class="navbar-toggler d-none" data-toggle="collapse"
                                    data-target="#navcol-1"><span class="sr-only">Toggle navigation</span><span
                                        class="navbar-toggler-icon"></span></button>
                                <div class="collapse navbar-collapse" id="navcol-1">
                                    <ul class="nav navbar-nav w-100 justify-content-between">
                                        <li class="nav-item active">
                                            <a class="nav-link" href="/">Trang chủ <span
                                                    class="sr-only">(current)</span></a>
                                        </li>
                                        {{#each this.arrCat}}
                                        {{#with category as | cat |}}
                                        <li class="nav-item dropdown"><a class="dropdown-toggle nav-link"
                                                data-toggle="dropdown" aria-expanded="false"
                                                href="/newspaper/bycate/{{cat.id}}">{{cat.name}}</a>
                                            <div class="dropdown-menu" role="menu">
                                                {{#each topic}}
                                                <a class="dropdown-item" role="presentation"
                                                    href="/newspaper?cat={{cat.id}}&&topic={{id}}">{{name}}</a>
                                                {{/each}}
                                            </div>
                                        </li>
                                        {{/with}}
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        </nav>

                        <!-- Nav End -->
                    </div>
                </nav>
            </div>
        </div>
    </div>
</header>
<!-- ##### Header Area Start ##### -->

<!-- ##### Header Area End ##### -->

<!-- ##### Hero Area Start ##### -->

<!-- ##### Hero Area End ##### -->

<!-- ##### Blog Area Start ##### -->
<div class="blog-area section-padding-0-80">

    <div class="container">
        <h4 style="margin: 30px 30px;">Kết quả tìm kiếm: <a style="font-size: 30px;" href="/newspaper?cat={{namecat.id}}">{{namecat.name}}</a> <a style="font-size: 30px;" href="/newspaper?topic={{nametopic.id}}">{{nametopic.name}}</a>  {{nametag}} </h4>
        <div class="row">
            <div class="col-12 col-lg-8">
                <div class="blog-posts-area">
                    <!-- Single Featured Post -->
                    {{#each items}}
                    <a style="margin-bottom: 90px;" href="/newspaper/detail/{{id}}">
                        <div class="single-blog-post featured-post ">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="post-thumb">
                                        <img src="{{avatar}}" style="width: 220px;height:148px;{{img_premium ispremium ../user.time_premium}}" alt="">
                                    </div>
                                </div>
                                <div class="col-sm-8">
                                    <div class="post-data" style="padding: 0;">
                                        <a href="/newspaper?topic={{topicid}}" class="post-catagory" style="margin: 0;">{{topic}}</a>
                                        <a href="/newspaper/detail/{{id}}" class="post-title">
                                            <h6 style="font-size: 20px;margin:0">{{name}}<span style="color: yellow;"> &nbsp;&nbsp;&nbsp; {{{content_premium ispremium}}}</span></h6>
                                             <p class="post-date"><span>Ngày đăng: {{format_date time}}  </span></p>
                                        </a>
                                        <div class="post-meta">
                                            <p class="post-author" style="margin: 0;">By <a href="#">{{writer}}</a></p>
                                            <p class="post-excerp" style="margin: 0;font-size: 11px;">{{desc}}</p>

                                            <div class="d-flex align-items-center" style="margin: 0;">
                                                {{#each tags}}
                                                <a style="font-size: 10px;" href="/newspaper?tag={{id}}"
                                                    class="post-like">
                                                    <span style="margin:0px;color:#3D7EF1">#{{name}}</span></a>
                                                {{/each}}
                                            </div>
                                            <!-- Post Like & Post Comment -->
                                            <div class="d-flex align-items-center">
                                                <a href="/newspaper/detail/{{id}}" class="post-like"><img src="public/img/core-img/eye.png" alt="">
                                                    <span style="font-size: 12px;">{{view}}</span></a>
                                                {{!-- <a href="#" class="post-comment"><img src="/public/img/chat.png" alt="">
                                                    <span style="font-size: 12px;">{{comment}}</span></a> --}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </a>
                    {{/each}}
                </div>
                {{#if issearch}}
                <nav aria-label="Page navigation" >
                    <ul class="pagination justify-content-center" >
                        <li  class="page-item {{prev.css}}" ><a  class="page-link" onclick="{{prev.onclick}}"
                                href="/newspaper/search?typeahead={{tag}}&&page=1">&lt;&lt;</a></li>
                        
                        <li class="page-item {{prev.css}}">
                            <a class="page-link " onclick="{{prev.onclick}}"
                                href="/newspaper/search?typeahead={{tag}}&&page={{#add page -1}}{{/add}}" tabindex="-1">&lt;</a>
                        </li>
                        {{#each pages}}
                        <li class="page-item {{active}}"><a class="page-link"
                                href="/newspaper/search?typeahead={{../tag}}&&page={{index}}">{{index}}</a></li>
                        {{/each}}
                        <li class="page-item {{next.css}}">
                            <a class="page-link " onclick="{{next.onclick}}"
                                href="/newspaper/search?typeahead={{tag}}&&page={{#add page 1}}{{/add}}">&gt;</a>
                        </li>
                        <li class="page-item {{next.css}}"><a class="page-link" onclick="{{next.onclick}}"
                                href="/newspaper/search?typeahead={{tag}}&&page={{last}}">&gt;&gt;</a></li>
                    </ul>
                </nav>
                {{else}}
                {{#if istag}}
                <nav aria-label="Page navigation" >
                    <ul class="pagination justify-content-center" >
                        <li  class="page-item {{prev.css}}" ><a  class="page-link" onclick="{{prev.onclick}}"
                                href="/newspaper?tag={{tag}}&&page=1">&lt;&lt;</a></li>
                        
                        <li class="page-item {{prev.css}}">
                            <a class="page-link " onclick="{{prev.onclick}}"
                                href="/newspaper?tag={{tag}}&&page={{#add page -1}}{{/add}}" tabindex="-1">&lt;</a>
