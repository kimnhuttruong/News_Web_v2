<link rel="stylesheet" href="/public/style.css">
<header class="header-area">
  <!-- Top Header Area -->
  <div class="top-header-area">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <div class="top-header-content d-flex align-items-center justify-content-between">
            <!-- Logo -->
            <div class="logo">
              <a href="/"><img
                  src="https://w7.pngwing.com/pngs/982/544/png-transparent-news-graphy-logo-icon-news-logo-text-photography-computer-wallpaper.png"
                  alt=""></a>
            </div>

            <!-- Login Search Area -->
            <div class="login-search-area d-flex align-items-center">
              <!-- Login -->
              <div class="login d-flex">

                {{#if isLogin}}
                <a href="/account/login">Login</a>
                {{else}}
                <a href="/account/login">Logout</a>
                {{/if}}
                <a href="/account/register">Register</a>
              </div>
              <!-- Search Form -->
              <div class="search-form">
                <form action="/search" method="post">
                  <div class="bs-example">
                    <input type="search" class="typeahead tt-query" name="typeahead" placeholder="">
                  </div>
                  <button type="submit"><i class="fa fa-search" aria-hidden="true"></i></button>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

